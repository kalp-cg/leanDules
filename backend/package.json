{
  "name": "learnduels-backend",
  "version": "1.0.0",
  "description": "Backend for LearnDuels - Educational Quiz Platform",
  "main": "src/server.js",
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit -i",
    "dev": "nodemon --watch src --exec node src/server.js",
    "start": "nodemon --watch src --exec node src/server.js",
    "build": "npx prisma generate",
    "postinstall": "npx prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "node prisma/seed.js"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "education",
    "quiz",
    "real-time",
    "nodejs",
    "express"
  ],
  "author": "LearnDuels Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cheerio": "^1.1.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-prom-bundle": "^7.0.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.3.1",
    "firebase-admin": "^13.6.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.11",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "pg": "^8.16.3",
    "prisma": "^5.22.0",
    "prom-client": "^15.1.0",
    "rate-limit-redis": "^4.2.0",
    "socket.io": "^4.8.1",
    "@socket.io/redis-adapter": "^8.3.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "artillery": "^2.0.3",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4"
  }
}